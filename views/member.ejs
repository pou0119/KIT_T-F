    <body>
      <div id="header">
       <script>
           fetch("/header.ejs")
               .then(response => response.text())
               .then(data => {
                   document.querySelector("#header").innerHTML = data;
               })
               .catch(error => console.error('Error fetching header:', error));
       </script>
      </div>


      
      <main class="backgraund">
        <div id="member">
          <h2>選手紹介</h2>   
        </div>
      </main>
     

      <!-- フッターの埋め込み -->
      <div id="footer">
